{
    "name": "kit.website",
    "packageManager": "yarn@3.2.2",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "atlr.kit": "*",
        "next": "^13.1.5",
        "next-themes": "^0.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^13.1.5",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "eslint": "^8.32.0",
        "eslint-config-kit": "*",
        "eslint-config-next": "^13.1.5",
        "prettier": "^2.8.3",
        "typescript": "^4.9.4"
    },
    "prettier": {
        "bracketSameLine": true,
        "bracketSpacing": false,
