{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["build/**", "dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "release": {
      "outputs": ["build/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    }
  }
}
