{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": ["build/**", ".next/**"],
            "dependsOn": ["^build"]
        },
        "dev": {
            "dependsOn": ["^build"],
            "outputs": [".next/**"],
            "inputs": ["src/**/*.tsx", "src/**/*.ts"]
        },
        "release": {
            "outputs": ["build/**"],
            "dependsOn": ["^build"]
        },
        "lint": {
            "outputs": []
        }
    }
}